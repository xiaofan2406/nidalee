{
  "name": "nidalee",
  "version": "0.0.16",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "keywords": [
    "nidalee",
    "react",
    "UI"
  ],
  "repository": "xiaofan2406/nidalee",
  "bugs": {
    "url": "https://github.com/xiaofan2406/nidalee/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "dist/commonjs",
  "module": "dist/es6",
  "sideEffects": false,
  "scripts": {
    "lint:eslint": "eslint '**/*.js' --ignore-path .gitignore",
    "lint:prettier": "prettier-check '**/*.{js,json,ts,tsx,md}' --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "format": "prettier '**/*.{js,json,ts,tsx,md}' --write --ignore-path .gitignore",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "prebuild": "yarn lint && yarn flow && rm -rf dist/es6 dist/commonjs",
    "build:commonjs": "NODE_ENV=production babel src --out-dir dist/commonjs --ignore 'src/**/__tests__/'",
    "build:es6": "NODE_ENV=production MODULE=es6 babel src --out-dir dist/es6 --ignore 'src/**/__tests__/'",
    "build": "yarn build:commonjs && yarn build:es6",
    "dev": "NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js",
    "predocs": "rm -rf ./dist/docs",
    "docs": "NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "start": "serve ./dist/docs -s",
    "docs:publish": "yarn docs && surge -p ./dist/docs -d nidalee.surge.sh",
    "prepublishOnly": "yarn build",
    "postpublish": "git push --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "./config/jest/jest.config.json"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "dependencies": {
    "color": "^3.0.0",
    "emotion": "^9.2.12",
    "react-emotion": "^9.2.12"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-prismjs": "^1.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "flow-bin": "^0.82.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "jest-emotion": "^9.2.11",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.14.3",
    "prettier-check": "^2.0.0",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "raw-loader": "^0.5.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.5.2",
    "serve": "^10.0.2",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}

{
  "name": "nidalee",
  "version": "0.0.19",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "Nidalee UI framework for React",
  "keywords": [
    "nidalee",
    "react",
    "UI"
  ],
  "repository": "xiaofan2406/nidalee",
  "bugs": {
    "url": "https://github.com/xiaofan2406/nidalee/issues"
  },
  "license": "MIT",
  "engines": {
    "node": "^14 || ^16",
    "npm": "^8"
  },
  "scripts": {
    "format": "prettier '**/*.{js,jsx,json,ts,tsx,md,mdx}' --write --ignore-path .gitignore",
    "lint:eslint": "eslint '**/*.{ts,tsx,js,jsx}' --ignore-path .gitignore",
    "lint:prettier": "prettier '**/*.{js,jsx,json,ts,tsx,md,mdx}' --check --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "prepare": "husky install",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "new": "node ./scripts/generator/main",
    "dev": "vite",
    "type-check": "tsc --noEmit --project tsconfig.build.json",
    "build:src": "npm run type-check && vite build",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "build": "npm run build:src && npm run build:declaration",
    "postbuild": "node ./scripts/postBuild.js",
    "postpublish": "git push && git push --tags"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/nidalee.umd.js",
  "module": "./dist/nidalee.es.js",
  "exports": {
    ".": {
      "import": "./dist/nidalee.es.js",
      "require": "./dist/nidalee.umd.js"
    },
    "./style.css": "./dist/style.css",
    "./theme.css": "./dist/theme.css",
    "./reset.css": "./dist/reset.css"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx,md,mdx}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.8.0",
    "eslint-config-free": "^1.4.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-testing-library": "^5.0.4",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "postcss": "^8.4.5",
    "postcss-nesting": "^10.1.2",
    "postcss-preset-env": "^7.2.3",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.5.1",
    "react-router-dom": "^6.2.1",
    "tailwindcss": "^3.0.17",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "dependencies": {
    "react-feather": "^2.0.9"
  }
}

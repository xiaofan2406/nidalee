{
  "name": "nidalee",
  "version": "0.0.16",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "keywords": ["nidalee", "react", "UI"],
  "repository": "xiaofan2406/nidalee",
  "bugs": {
    "url": "https://github.com/xiaofan2406/nidalee/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "dist/commonjs",
  "module": "dist/es6",
  "sideEffects": false,
  "scripts": {
    "precommit": "lint-staged",
    "lint:eslint": "eslint src config docs",
    "lint:prettier":
      "prettier-check '{src,config,docs}/**/*.{js,mjs,json,css,md}'",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "format": "prettier '{src,config,docs}/**/*.{js,mjs,json,css,md}' --write",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "prebuild": "yarn lint && yarn flow && rm -rf dist/es6 dist/commonjs",
    "build:commonjs":
      "NODE_ENV=production babel src --out-dir dist/commonjs --ignore 'src/**/__tests__/'",
    "build:es6":
      "NODE_ENV=production MODULE=es6 babel src --out-dir dist/es6 --ignore 'src/**/__tests__/'",
    "build": "npm run build:commonjs && npm run build:es6",
    "dev":
      "NODE_ENV=development webpack-dev-server --config ./config/webpack.dev.js",
    "predocs": "rm -rf ./dist/docs",
    "docs": "NODE_ENV=production webpack --config ./config/webpack.prod.js",
    "start": "node ./config/serveDocs.js",
    "docs:publish": "yarn docs && surge -p ./dist/docs -d nidalee.surge.sh",
    "prepublishOnly": "yarn build",
    "postpublish": "git push --follow-tags"
  },
  "lint-staged": {
    "*.{js,mjs,json,css,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "<rootDir>/config/jest/jest.config.json"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "dependencies": {
    "color": "^3.0.0",
    "emotion": "^9.1.2",
    "react-emotion": "^9.1.2"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-flow": "^7.0.0-beta.46",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.0-charlie.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-emotion": "^9.1.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "escape-string-regexp": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "filemanager-webpack-plugin": "^1.0.26",
    "flow-bin": "^0.71.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-emotion": "^9.1.2",
    "json-loader": "^0.5.7",
    "lint-staged": "^7.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "prettier": "^1.12.1",
    "prettier-check": "^2.0.0",
    "prismjs": "^1.14.0",
    "raw-loader": "^0.5.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.1.2",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.3.2",
    "serve": "^6.5.6",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  }
}

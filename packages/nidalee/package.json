{
  "name": "nidalee",
  "version": "0.0.19",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "Nidalee UI framework for React",
  "keywords": [
    "nidalee",
    "react",
    "UI"
  ],
